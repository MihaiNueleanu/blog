  </head>

  <body class="landing">
    
    {% include 'header.njk' %}

    <div class="content">

      <div class="latest-posts">
        <h2>Latest posts</h2>
        {%- for post in collections.published -%}
          <div class="latest-posts__item">
            <span class="date">{{post.date | readableDate}} </span>
            <a href="{{post.url}}">{{ post.data.title }}</a>
          </div>
        {%- endfor -%}
      </div>
      

      {% include 'brief.njk' %}
